{
  "name": "tmhub-design-system-react",
  "version": "0.0.1",
  "description": "TeamHub design system for react",
  "main": "./src/index.ts",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.3.1",
