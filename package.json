    "storybook": "^8.6.12",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.2.6"
  },
  "scripts": {
    "storybook": "storybook dev -p 6006 -h 0.0.0.0",
